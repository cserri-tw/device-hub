{
  "listeners": [
    {
      "type": "http",
      "uid": "http-one",
      "configuration": {
        "HTTPBindingAddress": ":8085"
      }
    },
    {
      "type": "mqtt",
      "uid": "mqtt-one",
      "configuration": {
        "MQTTBrokerAddress": "tcp://0.0.0.0:1883"
      }
    }
  ],
  "endpoints": [
    {
      "type": "stdout",
      "uid": "stdout-one"
    }
  ],
  "profiles": [
    {
      "uid": "xxx",
      "type": "script",
      "name": "some-device-profile",
      "description": "blah-blah",
      "version": "1.0.0-beta",
      "script": {
        "main": "decode",
        "runtime": "javascript",
        "input": "json",
        "contents": "function decode(x) { return x };"
      }
    }
  ],
  "pipes": [
    {
      "uri": "/a",
      "profile": "xxx",
      "listener": "http-one",
      "endpoints": ["stdout-one"]
    },
    {
      "uri": "/b",
      "profile": "xxx",
      "listener": "mqtt-one",
      "endpoints": ["stdout-one", "stdout-one"]
    }
  ]
}
